    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "ast-pretty-print": "^2.0.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-tester": "^9.0.1",
    "husky": "^8.0.1",
    "jest": "^26.0.1",
    "lint-staged": "^13.0.1",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.2",
